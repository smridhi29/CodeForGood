                </ul>
            </div>
        </div>
    </nav>
    <!--Content -->
    <table>
        <thead>
            <th>
                Mentee's Name
            </th>
            <th>
                Test Name
            </th>
            <th>
                Grades
            </th>
        </thead>
        <tbody>
            <% MenteeGrades.forEach((mentee)=>{ %>
                <tr>
                    <td> <%= mentee.Name %></td>
                    <td> <%= mentee.testName %></td>
                    <td> <%= mentee.grade %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
    <!--Content End-->
    <footer class="py-5 bg-dark">
